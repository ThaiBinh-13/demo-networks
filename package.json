{
  "name": "multiplier-checker",
  "version": "2.0.0",
  "private": true,
  "description": "Multiplier Checker Page (Vue 3)",
  "scripts": {
    "build": "vue-cli-service build",
    "dev": "vue-cli-service serve",
    "fix": "run-s fix:*",
    "fix:css": "stylelint src/**/*.{vue,tsx,css,scss} --allow-empty-input --fix",
    "fix:es": "vue-cli-service lint src/**/*.{yml,json,js,ts,tsx,vue}",
    "fix:prettier": "prettier ** --write",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/locales/**/*.json\"",
    "test": "run-s test:*",
    "test:css": "stylelint src/**/*.{vue,tsx,css,scss} --allow-empty-input",
    "test:es": "vue-cli-service lint --no-fix src/**/*.{yml,json,js,ts,tsx,vue}",
    "test:prettier": "prettier ** --list-different",
    "test:spelling": "cspell **",
    "types-gen": "typechain --target=web3-v1 src/abi/*/*.json --outDir ./src/typings/typechain"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "5.15.2",
    "@fortawesome/vue-fontawesome": "^3.0.0-3",
    "@polkadot/api": "^6.0.3",
    "@polkadot/extension-dapp": "^0.39.3",
    "@project-serum/borsh": "^0.2.2",
    "@solana/wallet-adapter-solflare": "^0.2.3",
    "@solana/web3.js": "^1.26.0",
    "@vueuse/core": "^4.9.1",
    "apollo-boost": "^0.4.9",
    "async": "^3.2.1",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.1",
    "buffer-layout": "^1.2.2",
    "core-js": "^3.11.2",
    "dayjs": "^1.10.4",
    "element-plus": "^1.0.2-beta.54",
    "graphql": "^15.5.1",
    "json-bigint": "^1.0.0",
    "lodash-es": "^4.17.21",
    "qs": "^6.10.1",
    "tiny-invariant": "^1.1.0",
    "typechain": "^5.1.2",
    "vue": "^3.2.0-beta.7",
    "vue-clipboard3": "^1.0.0",
    "vue-content-loader": "^2.0.0",
    "vue-i18n": "^9.1.6",
    "vue-router": "^4.0.0-0",
    "vue-router-layout": "^0.4.0",
    "web3": "^1.5.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@intlify/eslint-plugin-vue-i18n": "^0.11.1",
    "@intlify/vue-i18n-loader": "^2.1.0",
    "@typechain/web3-v1": "^3.0.0",
    "@types/async": "^3.2.7",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^15.0.1",
    "@types/tailwindcss": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "@vue/cli-plugin-babel": "^4.5.12",
    "@vue/cli-plugin-eslint": "^4.5.12",
    "@vue/cli-plugin-router": "^4.5.12",
    "@vue/cli-plugin-typescript": "^4.5.12",
    "@vue/cli-service": "^4.5.12",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "autoprefixer": "9.8.6",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.13.3",
    "cspell": "^5.3.12",
    "eslint": "^7.25.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.9.0",
    "husky": "^7.0.1",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "postcss": "7.0.35",
    "postcss-loader": "5.2.0",
    "prettier": "^2.3.0",
    "sass": "^1.32.12",
    "sass-loader": "^10.1.1",
    "style-resources-loader": "^1.4.1",
    "stylelint": "^13.13.1",
    "stylelint-config-hudochenkov": "^5.0.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@2.1.2",
    "typescript": "^4.2.4",
    "vue-auto-routing": "^1.0.1",
    "vue-cli-plugin-auto-routing": "^1.3.0",
    "vue-cli-plugin-i18n": "~2.1.0",
    "vue-cli-plugin-import-components": "~1.0.1",
    "vue-cli-plugin-webpack-bundle-analyzer": "~4.0.0"
  },
  "engines": {
    "node": ">=14"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint"
    ],
    "*.{vue,scss}": [
      "stylelint --fix"
    ]
  }
}
